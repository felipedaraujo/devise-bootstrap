<div class="row">
  <div class="container">
    <p id="notice"><%= notice %></p>

    <h3><%= simple_format(@protocol.title) %></h3>

    <p>
      <strong>Principal author:</strong>
      <%= @protocol.author %>
        
    </p>

    <p>
      <strong>Method:</strong>
      <% @protocol.method.each do |item| -%>
        <%= simple_format(item) %>
      <% end -%>
    </p>

    <p>
      <strong>Journal:</strong>
      <%= @protocol.journal %>
    </p>
    <div class="pull-right">
      <%= link_to 'Edit', edit_protocol_path(@protocol), class: "btn btn-primary" %>
      <%= link_to 'Back', protocols_path, class: "btn btn-default" %>
    </div>
  </div>
</div>